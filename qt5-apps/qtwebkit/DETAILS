          MODULE=qtwebkit
         VERSION=5.8.0
          SOURCE=$MODULE-opensource-src-$VERSION.tar.xz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-opensource-src-$VERSION
      SOURCE_URL=https://download.qt.io/community_releases/${VERSION%.*}/$VERSION-final/
      SOURCE_VFY=sha256:79ae8660086bf92ffb0008b17566270e6477c8fa0daf9bb3ac29404fb5911bec
        WEB_SITE=http://qt-project.org/
         ENTERED=20160505
         UPDATED=20170128
           SHORT="Qt based webkit"

cat << EOF
A Qt based webkit. This used to be part of qt5 but has sense been removed.
Unfortuneatly, KDE frameworks in particular kdewebkit still requires qtwebkit.
EOF
