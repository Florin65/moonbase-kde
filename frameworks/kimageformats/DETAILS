          MODULE=kimageformats
         VERSION=5.81.0
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/frameworks/$(module_version kf5)
      SOURCE_VFY=sha256:a332a0d708417c6e973b19d8b3aec006f3c1c57ef17851dc00e4921d7c43864e
   MODULE_PREFIX=${KDE4_INSTALL_DIR:-/usr}
        WEB_SITE=https://projects.kde.org/projects/frameworks/kimageformats
         ENTERED=20150205
         UPDATED=20210411
           SHORT="allow QImage to support extra file formats"

cat << EOF
This framework provides additional image format plugins for QtGui.  As
such it is not required for the compilation of any other software, but
may be a runtime requirement for Qt-based software to support certain
image formats.
EOF
