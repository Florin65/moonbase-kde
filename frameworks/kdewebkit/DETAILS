          MODULE=kdewebkit
         VERSION=5.61.0
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/frameworks/$(module_version kf5)/portingAids/
      SOURCE_VFY=sha256:1ee2a00ee3d95df9270e8c3d434568cda8f42151e361bc07fe374bf0f7afe211
