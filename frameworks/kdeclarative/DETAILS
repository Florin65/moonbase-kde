          MODULE=kdeclarative
         VERSION=5.80.0
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/frameworks/$(module_version kf5)
      SOURCE_VFY=sha256:bf55ba32475f3459e32f38ca49ced27373427b679ab5dd98e03f02e6f1359d3d
