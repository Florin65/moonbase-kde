          MODULE=kdeclarative
         VERSION=5.61.0
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/frameworks/$(module_version kf5)
      SOURCE_VFY=sha256:464a77f88cce72c1616654c371068c11d51e484e0de5c0c5e032126d71afedaa
