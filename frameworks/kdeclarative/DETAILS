          MODULE=kdeclarative
         VERSION=5.23.0
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/frameworks/$(module_version kf5)
      SOURCE_VFY=sha256:fba8b19971bfa72fc20bf79a324d0be85c7f728837ad7ef349c6a339d8f831ff
