          MODULE=kdesignerplugin
         VERSION=5.42.0
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/frameworks/$(module_version kf5)
      SOURCE_VFY=sha256:66c5119cb2687df988e21999b1600282005e1bfa899e5e4c2325c9e982ea8a00
   MODULE_PREFIX=${KDE4_INSTALL_DIR:-/usr}
        WEB_SITE=https://projects.kde.org/projects/frameworks/kdesignerplugin
         ENTERED=20150205
         UPDATED=20180114
           SHORT="Integrating KDE frameworks widgets with Qt Designer"

cat << EOF
This framework provides plugins for Qt Designer that allow it to display
the widgets provided by various KDE frameworks, as well as a utility
(kgendesignerplugin) that can be used to generate other such plugins
from ini-style description files.
EOF
