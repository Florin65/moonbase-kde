          MODULE=kded
         VERSION=5.11.0
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/frameworks/$(module_version kf5)
      SOURCE_VFY=sha256:59c602185df4ce6d146666052e01b05cdeb0579dd143a82fcbf22647a5ee3370
   MODULE_PREFIX=${KDE4_INSTALL_DIR:-/usr}
        WEB_SITE=https://projects.kde.org/projects/frameworks/kded
         ENTERED=20150205
         UPDATED=20150626
           SHORT="central daemon of KDE work spaces"

cat << EOF
KDED stands for KDE Daemon which isn't very descriptive.
KDED runs in the background and performs a number of small tasks.
Some of these tasks are built in, others are started on demand.
EOF
