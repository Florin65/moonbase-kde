          MODULE=libktorrent
         VERSION=2.1.1
          SOURCE=$MODULE-$VERSION.tar.gz
 SOURCE_URL_FULL=https://github.com/KDE/libktorrent/archive/v$VERSION.tar.gz
   MODULE_PREFIX=${KDE4_INSTALL_DIR:-/usr}
      SOURCE_VFY=sha256:897d970d8e06c82dfb43757b23e3079731dbb74bca259c559e25b7d574dfece4
        WEB_SITE=https://www.kde.org/applications/internet/ktorrent/
         ENTERED=20100526
         UPDATED=20191115
           SHORT="BitTorrent library"

cat << EOF
Libktorrent contains all the torrent downloading code. The goal is to make libktorrent an independent library
(though still closely related to ktorrent), which can be used by other applications.
EOF
