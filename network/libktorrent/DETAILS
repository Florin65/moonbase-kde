          MODULE=libktorrent
         VERSION=2.1
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/ktorrent/$(module_version ktorrent | cut -c 1-3)
   MODULE_PREFIX=${KDE4_INSTALL_DIR:-/usr}
      SOURCE_VFY=sha256:3eefdcceec73410c558e82d7be22eb2ed18679e9ab64f34cc100b54e186fe26f
        WEB_SITE=http://ktorrent.pwsp.net/
         ENTERED=20100526
         UPDATED=20171203
           SHORT="BitTorrent library"

cat << EOF
Libktorrent contains all the torrent downloading code.
The goal is to make libktorrent an independent library
(though still closely related to ktorrent),
which can be used by other applications.
EOF
