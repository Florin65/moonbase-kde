          MODULE=kdevplatform
         VERSION=1.7.0
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/kdevelop/$(module_version kdevelop)/src
      SOURCE_VFY=sha1:9fc196e7cb09ab33fd5cfbf5af19aa7c513efdc9
   MODULE_PREFIX=${KDE_PREFIX:-/usr}
