          MODULE=kdeartwork
         VERSION=15.04.3
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/applications/$VERSION/src
      SOURCE_VFY=sha256:3709113f701cced651b1cc5fe90a277b791f6281a959eb36692ff9682defc8ab
   MODULE_PREFIX=${KDE4_INSTALL_DIR:-/usr}
        WEB_SITE=http://www.kde.org
         ENTERED=20071026
         UPDATED=20150702
           SHORT="kde artwork that used to be in kdelibs"

cat << EOF
This package contains additional

* themes,
* screensaver,
* sounds,
* wallpapers,
* widget styles and
* window styles

for KDE. We placed them into this module so that kdebase won't be too bloated.

EOF
