          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/applications/$VERSION/src
      SOURCE_VFY=sha256:980770b2a4a52d2053b84c0c6dc1e26d4f63e71ec1ccfb2ed4affe6c268828ca
