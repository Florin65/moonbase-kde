          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/release-service/$VERSION/src
      SOURCE_VFY=sha256:a3e8b6aad1792f27d9fba69bc2abf2c84118f9667f0abf693892bccf5325f49c
