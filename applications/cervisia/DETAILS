          MODULE=cervisia
         VERSION=19.08.0
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/applications/$VERSION/src
      SOURCE_VFY=sha256:a72c3a7bad3b30a466d6793318248329d7ddb1a0e00c832744b628a9eb8b8257
