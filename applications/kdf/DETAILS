          MODULE=kdf
         VERSION=17.08.3
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/applications/$VERSION/src
      SOURCE_VFY=sha256:986f299e71de1e36ae9738ecede3767d207f912874e24c2f17c987f2080b49e8
