          MODULE=juk
         VERSION=20.08.0
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/release-service/$VERSION/src
      SOURCE_VFY=sha256:ec00ef054768efc64b5b0da69fba90104689b314a064f52989d1c6dbd73dad1b
