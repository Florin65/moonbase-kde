         VERSION=16.04.2
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/applications/$VERSION/src
      SOURCE_VFY=sha256:7b2438a2e65ae3f2f7e09324c783d2bde8d9012a12e89e247d9211c886b4c315
