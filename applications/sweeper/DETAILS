          MODULE=sweeper
         VERSION=18.08.2
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/applications/$VERSION/src
      SOURCE_VFY=sha256:f5bdf52c5f6c4921f4180da8cbbfb17bfd40edb98a43c4040b16f0216b201e70
