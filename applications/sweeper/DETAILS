          MODULE=sweeper
         VERSION=17.08.3
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/applications/$VERSION/src
      SOURCE_VFY=sha256:b61bb0e9e86f3aeeedb5aae4b7d6d909cd7bf26dd6df2e3cd29a7b22c09fd54b
