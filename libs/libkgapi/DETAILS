          MODULE=libkgapi
         VERSION=2.1.1
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=$KDE_URL/stable/$MODULE/$VERSION/src/
      SOURCE_VFY=sha1:98a9a657238333a79ee916c9071d32e20d54e0b7
        WEB_SITE=http://www.progdan.cz/2012/05/libkgoogle-libkgapi/
