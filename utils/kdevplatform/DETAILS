          MODULE=kdevplatform
         VERSION=1.3.1
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=$KDE_URL/stable/kdevelop/$(module_version kdevelop)/src
      SOURCE_VFY=sha1:bba0cefdac6de83c8d3354534a94e8500dccda37
   MODULE_PREFIX=${KDE_PREFIX:-/usr}
        WEB_SITE=http://www.kde.org
         ENTERED=20090824
         UPDATED=20120414
           SHORT="create KDE plugins"

cat << EOF
The idea is that kdevplatform contains:
    * Everything to create platform plugins
    * Everything to create platform applications
    * Commonly used/important plugins
EOF
