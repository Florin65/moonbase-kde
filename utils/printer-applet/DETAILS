          MODULE=printer-applet
         VERSION=4.9.3
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/$VERSION/src
      SOURCE_VFY=sha1:841bb5136604d26a6c70c25b29aa245e7c9d09b9
   MODULE_PREFIX=${KDE4_INSTALL_DIR:-/usr}
        WEB_SITE=http://netdragon.sourceforge.net/ssuperkaramba.html
         ENTERED=20120125
         UPDATED=20121106
           SHORT="system tray utility to show current print jobs"

cat << EOF
printer-applet is a system tray utility.  It shows current print jobs,
shows printer warnings and errors and shows when printers that have
been plugged in for the first time are being auto-configured by
hal-cups-utils.
EOF
